{
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[typescriptreact]": {
    // "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontFamily": "JetBrains Mono",
  "editor.lineHeight": 1.8,
  "editor.renderLineHighlight": "gutter",
  "editor.fontLigatures": true, 
  "editor.fontVariations": false,
  "editor.fontWeight": "normal",
  "editor.minimap.enabled": false,
  "editor.unicodeHighlight.allowedCharacters": {
    "Ë†": true
  },
  "apc.electron": {
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 11,
      "y": 10
    }
  },
  "apc.listRow": {
    "height": 24
  },
  "apc.stylesheet": {
    ".title-label > h2": "display: none"
  },
  "window.titleBarStyle": "native",
  "breadcrumbs.enabled": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "git.autofetch": true,
  "git.suggestSmartCommit": false,
  "gitlens.views.remotes.branches.layout": "list",
  "liveServer.settings.donotShowInfoMsg": true,
  "symbols.hidesExplorerArrows": false,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm",
  "terminal.integrated.automationProfile.osx": {},
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.lineHeight": 1.6,
  "terminal.integrated.rightClickBehavior": "paste",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "symbols",
  "workbench.preferredDarkColorTheme": "Visual Studio Light",
  "workbench.preferredHighContrastColorTheme": "Tomorrow Night Blue",
  "workbench.preferredHighContrastLightColorTheme": "Abyss",
  "workbench.preferredLightColorTheme": "Default Dark+",
  "workbench.settings.applyToAllProfiles": [
    "editor.fontSize"
  ],
  "workbench.startupEditor": "none",
  "screencastMode.fontSize": 40,
  "editor.fontSize": 13.5,
  "workbench.tree.indent": 4,
  "workbench.editor.decorations.badges": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.colorTheme": "Min Dark",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false
}
